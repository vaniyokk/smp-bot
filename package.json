{
  "name": "sheet-music-publishing-bot",
  "version": "1.0.0",
  "description": "Automated solution for publishing sheet music from Notion to website with AI-enhanced metadata and YouTube integration",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "prepare": "husky"
  },
  "keywords": [
    "automation",
    "sheet-music",
    "notion",
    "playwright",
    "ai",
    "youtube-api",
    "publishing"
  ],
  "author": "vaniyokk",
  "license": "MIT",
  "dependencies": {
    "@notionhq/client": "^2.2.15",
    "@playwright/test": "^1.45.0",
    "dotenv": "^16.4.5",
    "googleapis": "^140.0.1",
    "openai": "^4.57.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.34.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "rimraf": "^5.0.8",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.40.0",
    "vitest": "^2.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --max-warnings 0"
    ]
  }
}